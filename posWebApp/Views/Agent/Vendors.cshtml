<script src="https://code.jquery.com/ui/1.13.1/jquery-ui.js"></script>
<script>
    $(document).ready(function () {
        $(".a-enter").click(function () {
            //Get the id of the selected item in dropdown
            var id = $('#agentId').val();

            //Replace the predifined QueryString param "xxx" with the "id"
            this.href = this.href.replace("xxx", id);
        });

        $("#agentName").autocomplete({
            source: function (request, response) {
                 var customer = new Array();
                $.ajax({
                    url: "/Agent/GetVendors",
                    type: "POST",
                    dataType: "json",
                    async: true,
                    data: { Prefix: request.term },
                    success: function (data) {
                      response($.map(data, function (item) {
                          return {
                              label: item.label,
                              value: item.label,
                              ID: item.val
                        };
                    }));

                    }
                })
            },
            select: function (event, ui) {
                $('#agentId').val(ui.item.ID);
                $('#agentName').text(ui.item.label);
                refreshFunc();
        },
           messages: {
               noResults: "", results:function(resultsCount) {}
           }
        });

    });

    function refreshFunc() {
        val = $('#agentId').val();

        if (val != '') {
            $.ajax({
                url: '@Url.Action("GetAgentInfo", "Agent")',
                data: { agentId: $('#agentId').val() },
                type: "POST",
                async: false,
                success: function (result) {

                    $('#pName').html('<span>@AppResource.Name: </span>' + result.name);
                    $('#pCompany').html('<span>@AppResource.Company: </span>' + result.company);
                    $('#pMobile').html('<span>@AppResource.Mobile: </span>' + result.mobile);
                    $('#statement').html('<span>'+result.balanceType +' </span>' + result.balance);
                    $('.p-enter').show();

                    if (result.image != null && result.image != '') {
                        $('#img_Customer').attr('src', result.image)
                    }
                    else {
                        $('#img_Customer').attr('src',"@Url.Content("~/"+Session["lang"].ToString()+"/images/avatar.jpg")");
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert('oops, something bad happened');
                }
            });
        }
        else {
             $('#pName').html('<span>@AppResource.Name: </span>');
             $('#pCompany').html('<span>@AppResource.Company: </span>');
             $('#pMobile').html('<span>@AppResource.Mobile: </span>');

             $('#img_Customer').attr('src',"@Url.Content("~/"+Session["lang"].ToString()+"/images/avatar.jpg")");

             $('.p-enter').hide();

        }
        }
</script>