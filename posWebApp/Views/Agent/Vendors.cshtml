                        @Html.TextBox("agentName", Model.name, new { @class = "form-control", Id = "agentName", name = "agentName" })
                        <input type="hidden" id="agentId" name="agentId" value="@Model.agentId" />
                        @*@Html.DropDownListFor(m => m.agentId, new SelectList(ViewBag.vendors, "agentId", "name"), string.Empty, new { @class = "form-control", name = "agentId", Id = "agentId", @onChange = "refreshFunc();" })*@

